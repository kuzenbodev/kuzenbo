{
  "name": "@kuzenbo/cli",
  "version": "0.0.3",
  "private": true,
  "description": "Placeholder package for future Kuzenbo CLI tooling.",
  "keywords": [
    "cli",
    "kuzenbo",
    "tooling"
  ],
  "homepage": "https://github.com/kuzenbodev/kuzenbo#readme",
  "bugs": {
    "url": "https://github.com/kuzenbodev/kuzenbo/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuzenbodev/kuzenbo.git",
    "directory": "packages/cli"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "sideEffects": false,
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsgo --noEmit --declaration false --declarationMap false",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "catalog:",
    "bun-types": "catalog:",
    "publint": "catalog:",
    "typescript": "catalog:"
  }
}
