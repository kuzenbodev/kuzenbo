{
  "name": "@kuzenbo/styles",
  "version": "0.0.3",
  "private": false,
  "description": "Optional global baseline styles for Kuzenbo apps.",
  "keywords": [
    "component-library",
    "design-system",
    "kuzenbo",
    "react",
    "tailwind",
    "ui"
  ],
  "homepage": "https://github.com/kuzenbodev/kuzenbo#readme",
  "bugs": {
    "url": "https://github.com/kuzenbodev/kuzenbo/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuzenbodev/kuzenbo.git",
    "directory": "packages/styles"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    "./styles.css": "./src/recommended.css",
    "./recommended.css": "./src/recommended.css",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./styles.css": "./dist/recommended.css",
      "./recommended.css": "./dist/recommended.css",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "dependencies": {
    "tailwindcss": "catalog:"
  }
}
